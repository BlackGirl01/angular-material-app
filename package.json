{
  "name": "angular-material-app",
  "description": "基于最新Angular 6框架与Material 2技术的企业级通用管理系统快速开发框架，涵盖了大量的常用组件和基础功能，您可以快速开发各种MIS系统，如CMS、OA、CRM、ERP、POS等，最大程度上帮助企业节省时间成本和费用开支。",
  "homepage": "https://github.com/stbui/angular-material-app",
  "bugs": "https://github.com/stbui/angular-material-app/issues",
  "version": "0.6.0",
  "license": "MIT",
  "main": "dist/electron.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --extract-css",
    "start:proxy": "ng serve --extract-css --proxy-config proxy.conf.js",
    "build": "ng build --extract-css --prod --aot --environment=prod",
    "build:github": "ng build --extract-css --prod --aot --environment=github --base-href=/angular-material-app/",
    "publish:github": "ngh",
    "ngsw-config": "./node_modules/.bin/ngsw-config dist src/ngsw-config.json",
    "ngsw-copy": "cp node_modules/@angular/service-worker/ngsw-worker.js dist/",
    "build-prod-ngsw": "ng build --extract-css --prod --aot --environment=ngsw && npm run ngsw-config && npm run ngsw-copy",
    "build-github-ngsw": "npm run build:github && npm run ngsw-config && npm run ngsw-copy",
    "start:electron": "electron .",
    "electron-copy": "cp ./scripts/electron/electron.js dist/",
    "build-prod-electron": "ng build --extract-css --prod --aot --environment=prod --base-href=./ && npm run electron-copy",
    "build-pack-electron": "node ./scripts/electron/build.js",
    "docs": "hads ./docs -o",
    "compodoc": "./node_modules/.bin/compodoc -p tsconfig.json -s",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.0-beta.7",
    "@angular/cdk": "^6.0.0-beta.4",
    "@angular/common": "^6.0.0-beta.7",
    "@angular/compiler": "^6.0.0-beta.7",
    "@angular/core": "^6.0.0-beta.7",
    "@angular/flex-layout": "^5.0.0-beta.13",
    "@angular/forms": "^6.0.0-beta.7",
    "@angular/http": "^6.0.0-beta.7",
    "@angular/material": "^6.0.0-beta.4",
    "@angular/platform-browser": "^6.0.0-beta.7",
    "@angular/platform-browser-dynamic": "^6.0.0-beta.7",
    "@angular/router": "^6.0.0-beta.7",
    "@angular/service-worker": "^6.0.0-beta.7",
    "@swimlane/ngx-charts": "^7.1.1",
    "@swimlane/ngx-datatable": "^11.2.0",
    "@swimlane/ngx-dnd": "^3.1.3",
    "angular-calendar": "^0.22.3",
    "angularfire2": "^5.0.0-rc.6.0",
    "core-js": "^2.5.3",
    "d3": "^4.13.0",
    "firebase": "^4.9.0",
    "hammerjs": "^2.0.8",
    "material-design-icons": "^3.0.1",
    "ng2-charts": "^1.6.0",
    "ngx-perfect-scrollbar": "^5.3.5",
    "ngx-quill": "^2.1.2",
    "normalize.css": "^7.0.0",
    "rxjs": "^5.5.7",
    "screenfull": "^3.3.2",
    "zone.js": "^0.8.20"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.3",
    "@angular/compiler-cli": "^6.0.0-beta.7",
    "@angular/language-service": "^6.0.0-beta.7",
    "@compodoc/compodoc": "^1.0.9",
    "@types/node": "^6.0.102",
    "codelyzer": "^3.2.2",
    "hads": "^1.6.1",
    "protractor": "^5.3.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.9.1",
    "typescript": "^2.5.3"
  }
}
